<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="spacetrader.UniverseScreenController">
    <stylesheets>
        <URL value="@universescreen.css" />
    </stylesheets>
   <children>
      <BorderPane fx:id="MenuButton" onMouseReleased="#ReturnMainMenu" prefHeight="400.0" prefWidth="600.0">
         <center>
            <ImageView fitHeight="403.0" fitWidth="645.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER">
               <image>
                  <Image url="@stars.png" />
               </image>
            </ImageView>
         </center>
         <bottom>
            <MenuBar opacity="0.78" BorderPane.alignment="CENTER">
              <menus>
                <Menu mnemonicParsing="false">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Close" />
                  </items>
                     <graphic>
                        <Button mnemonicParsing="false" onMouseClicked="#SaveGame" text="Save">
                           <effect>
                              <ColorAdjust brightness="-0.38" />
                           </effect>
                        </Button>
                     </graphic>
                </Menu>
                <Menu mnemonicParsing="false">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Delete" />
                  </items>
                     <graphic>
                        <Button mnemonicParsing="false" onMouseClicked="#exit" text="Exit">
                           <effect>
                              <ColorAdjust brightness="-0.38" />
                           </effect>
                        </Button>
                     </graphic>
                </Menu>
                <Menu fx:id="fuelTab" mnemonicParsing="false">
                  <items>
                    <MenuItem mnemonicParsing="false" text="About" />
                  </items>
                </Menu>
              </menus>
               <effect>
                  <ColorAdjust brightness="-0.05" contrast="0.36" hue="1.0" saturation="0.43" />
               </effect>
            </MenuBar>
         </bottom>
      </BorderPane>
      <GridPane prefHeight="400.0" prefWidth="600.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="10.0" prefWidth="90.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="165.0" minWidth="0.0" prefWidth="110.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <ImageView fx:id="gal1" fitHeight="95.0" fitWidth="95.0" onMouseClicked="#goToGal1" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@galexy.png" />
               </image>
            </ImageView>
            <ImageView fx:id="gal4" fitHeight="150.0" fitWidth="200.0" onMouseClicked="#goToGal4" pickOnBounds="true" preserveRatio="true" GridPane.rowIndex="1">
               <image>
                  <Image url="@galexy3.png" />
               </image>
            </ImageView>
            <ImageView fx:id="gal7" fitHeight="77.0" fitWidth="73.0" onMouseClicked="#goToGal7" pickOnBounds="true" preserveRatio="true" GridPane.rowIndex="2">
               <image>
                  <Image url="@galexy7.png" />
               </image>
            </ImageView>
            <ImageView fx:id="gal5" fitHeight="124.0" fitWidth="89.0" onMouseClicked="#goToGal5" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <image>
                  <Image url="@galexy5.png" />
               </image>
            </ImageView>
            <ImageView fx:id="gal2" fitHeight="114.0" fitWidth="116.0" onMouseClicked="#goToGal2" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="1">
               <image>
                  <Image url="@galexy2.png" />
               </image>
            </ImageView>
            <ImageView fx:id="gal8" fitHeight="128.0" fitWidth="127.0" onMouseClicked="#goToGal8" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="1" GridPane.rowIndex="2">
               <image>
                  <Image url="@galexy8.png" />
               </image>
            </ImageView>
            <ImageView fx:id="gal3" fitHeight="106.0" fitWidth="84.0" onMouseClicked="#goToGal3" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="2">
               <image>
                  <Image url="@galexy4.png" />
               </image>
            </ImageView>
            <ImageView fx:id="gal6" fitHeight="150.0" fitWidth="200.0" onMouseClicked="#goToGal6" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="2" GridPane.rowIndex="1">
               <image>
                  <Image url="@galexy6.png" />
               </image>
            </ImageView>
            <ImageView fx:id="gal0" fitHeight="102.0" fitWidth="102.0" onMouseClicked="#goToGal0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="2" GridPane.rowIndex="2">
               <image>
                  <Image url="@galexy9.png" />
               </image>
            </ImageView>
         </children>
      </GridPane>
   </children>
</AnchorPane>
